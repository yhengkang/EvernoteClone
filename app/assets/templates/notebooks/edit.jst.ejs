<form class="form-inline" id="notebook-form">
	<div class="row">
	<div class="col-xs-8">
		<input class="form-control" type="text" name="notebook[name]" value="<%=notebook.escape("name")%>">
	</div>
	<button type="button" class="btn btn-sm" id="edit-notebook">
		<span class="glyphicon glyphicon-pencil"></span>
	</button>
	<button type="button" class="btn btn-sm" id="delete-notebook">
		<span class="glyphicon glyphicon-trash"></span>
	</button>
	</div>
</form>

